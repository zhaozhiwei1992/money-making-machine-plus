                <version>2.19.0</version>
            </dependency>

            <!--      https://github.com/imfangs/dify-java-client-->
            <dependency>
                <groupId>io.github.imfangs</groupId>
                <artifactId>dify-java-client</artifactId>
                <version>1.0.4</version>
            </dependency>

            <!-- 业务组件 -->
            <!--      这里按照需要, 现在版本都是跟着整体版本走, 没啥必要注册-->

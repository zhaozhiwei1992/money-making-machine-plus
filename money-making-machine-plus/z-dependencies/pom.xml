            <dependency>
                <groupId>com.github.binarywang</groupId>
                <artifactId>weixin-java-miniapp</artifactId>
                <version>4.4.8.B</version>
            </dependency>

            <!--    ai平台相关组件  -->
            <!-- https://mvnrepository.com/artifact/com.alibaba/dashscope-sdk-java -->
